{
  "name": "x-men-evolution/laravel-face-detection",
  "description": "Um pacote Laravel para detecção de faces e corte em imagens.",
  "license": "MIT",
  "keywords": ["Laravel", "Face Detection"],
  "authors": [{"name": "Arhey"}, {"name": "Seu Nome"}],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "intervention/image": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
